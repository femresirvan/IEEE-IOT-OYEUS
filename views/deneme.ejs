  <script>
    
    let x =Math.floor((Math.random() * 100) + 1);
    setInterval(() => {
      x=Math.floor((Math.random() * 100) + 1);
    }, 1000);
    function initMap(){
        const mapOptions = {
            zoom: 8,
            center:{lat:41.015137 , lng:28.979530}
        }
        const map = new google.maps.Map(document.getElementById('map'),mapOptions);
        
        const fireImageUrl = "http://maps.google.com/mapfiles/kml/pal3/icon42.png"

        const locationInfo = [
          {
            location: "A",
            coordinates: {lat:41.015137 , lng:28.979530},
            duman: x,
            pil:34
          },
          {
            location: "B",
            coordinates: {lat:41.015137 , lng:27.979530},
            duman: x,
            pil:23
          },
      ]

          locationInfo.forEach(info => {
            AddMarker({
              options: info
            });
          })
            
        function AddMarker({options}){

            const marker = new google.maps.Marker(
            {
                position: options.coordinates,
                map:map,
            })

            if(options.duman >50){
              marker.setIcon(fireImageUrl);
            }

            

             //infoWindow
             const { location, duman, pil } = options
            const InfoWindow= new google.maps.InfoWindow({
              content: `<h3> ${location} bölgesi</h3> <span>Duman yüzdesi:${duman}</span> <br>
              <span>Duman yüzdesi:${pil}</span>`
            });
            //click
            marker.addListener('click',function(){
              InfoWindow.open(map,marker);
            });
          
            }
        
    }
</script>
